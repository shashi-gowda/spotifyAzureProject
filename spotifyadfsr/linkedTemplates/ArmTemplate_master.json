{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spotifyadfsr"
		},
		"ls_azspotifydlake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_azspotifydlake'"
		},
		"ls_azsqlspotifydb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azsqlspotifydb'"
		},
		"ls_azspotifydlake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifystoragesr.dfs.core.windows.net/"
		},
		"ls_azsqlspotifydb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spotifyserversr.database.windows.net"
		},
		"ls_azsqlspotifydb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotifyDB"
		},
		"ls_azsqlspotifydb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsr"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spotifyadfsr_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_azspotifydlake_accountKey": {
						"value": "[parameters('ls_azspotifydlake_accountKey')]"
					},
					"ls_azsqlspotifydb_password": {
						"value": "[parameters('ls_azsqlspotifydb_password')]"
					},
					"ls_azspotifydlake_properties_typeProperties_url": {
						"value": "[parameters('ls_azspotifydlake_properties_typeProperties_url')]"
					},
					"ls_azsqlspotifydb_properties_typeProperties_server": {
						"value": "[parameters('ls_azsqlspotifydb_properties_typeProperties_server')]"
					},
					"ls_azsqlspotifydb_properties_typeProperties_database": {
						"value": "[parameters('ls_azsqlspotifydb_properties_typeProperties_database')]"
					},
					"ls_azsqlspotifydb_properties_typeProperties_userName": {
						"value": "[parameters('ls_azsqlspotifydb_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}